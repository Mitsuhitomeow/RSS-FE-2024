    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.2.0",
    "vite": "^5.1.4"
