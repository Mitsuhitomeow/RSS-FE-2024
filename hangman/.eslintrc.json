{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": ["error", { "ignore": [".svg$", ".mp3$"] }],
    "import/no-absolute-path": "off",
    "import/no-mutable-exports": "off",
    "import/no-cycle": "off",
    "semi": "error"
  }
}
